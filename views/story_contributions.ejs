  <body>
    <%- include('partials/_header') %>  
    <h1>Welcome!</h1>
      <p>This is a page to display all the story contributions</p>
      <form class="form-inline" action="/story_contributions" method="POST">
        <div class = "content"></div>
          Name: <input type="text" placeholder="Enter your name" name="name"><br/>
          Email: <input type="text" placeholder="Enter your email" name="email"><br/>
          Password: <input type="password" placeholder="Enter your password" name="password"><br/>
          Profile Picture: <input type="text" placeholder="Select profile picture" name="profile_pic_path"><br/>
          <input type="submit" value="Send"/>
        </div>
      </form>
    <footer>this is the footer</footer>
  </body>
